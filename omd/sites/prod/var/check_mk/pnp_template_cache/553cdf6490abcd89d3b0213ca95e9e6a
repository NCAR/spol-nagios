DEF:io_wait=$RRDBASE$_wait.rrd:1:MAX CDEF:io_wait_LEGSCALED=io_wait,1.000000,/ CDEF:io_wait_NEG=io_wait,-1,* CDEF:io_wait_LEGSCALED_NEG=io_wait_LEGSCALED,-1,* DEF:cpu_util_guest=$RRDBASE$_guest.rrd:1:MAX CDEF:cpu_util_guest_LEGSCALED=cpu_util_guest,1.000000,/ CDEF:cpu_util_guest_NEG=cpu_util_guest,-1,* CDEF:cpu_util_guest_LEGSCALED_NEG=cpu_util_guest_LEGSCALED,-1,* DEF:cpu_util_steal=$RRDBASE$_steal.rrd:1:MAX CDEF:cpu_util_steal_LEGSCALED=cpu_util_steal,1.000000,/ CDEF:cpu_util_steal_NEG=cpu_util_steal,-1,* CDEF:cpu_util_steal_LEGSCALED_NEG=cpu_util_steal_LEGSCALED,-1,* DEF:system=$RRDBASE$_system.rrd:1:MAX CDEF:system_LEGSCALED=system,1.000000,/ CDEF:system_NEG=system,-1,* CDEF:system_LEGSCALED_NEG=system_LEGSCALED,-1,* DEF:user=$RRDBASE$_user.rrd:1:MAX CDEF:user_LEGSCALED=user,1.000000,/ CDEF:user_NEG=user,-1,* CDEF:user_LEGSCALED_NEG=user_LEGSCALED,-1,* AREA:user#60f020:"User                   " LINE:user#4cc019 GPRINT:user_LEGSCALED:AVERAGE:"%8.2lf%% average" GPRINT:user_LEGSCALED:MAX:"%8.2lf%% max" GPRINT:user_LEGSCALED:LAST:"%8.2lf%% last" COMMENT:"\n" AREA:system#ff6000:"System                 ":STACK GPRINT:system_LEGSCALED:AVERAGE:"%8.2lf%% average" GPRINT:system_LEGSCALED:MAX:"%8.2lf%% max" GPRINT:system_LEGSCALED:LAST:"%8.2lf%% last" COMMENT:"\n" AREA:io_wait#00b0c0:"I/O-wait               ":STACK GPRINT:io_wait_LEGSCALED:AVERAGE:"%8.2lf%% average" GPRINT:io_wait_LEGSCALED:MAX:"%8.2lf%% max" GPRINT:io_wait_LEGSCALED:LAST:"%8.2lf%% last" COMMENT:"\n" AREA:cpu_util_guest#cc00ff:"Guest operating systems":STACK GPRINT:cpu_util_guest_LEGSCALED:AVERAGE:"%8.2lf%% average" GPRINT:cpu_util_guest_LEGSCALED:MAX:"%8.2lf%% max" GPRINT:cpu_util_guest_LEGSCALED:LAST:"%8.2lf%% last" COMMENT:"\n" AREA:cpu_util_steal#ffa000:"Steal                  ":STACK GPRINT:cpu_util_steal_LEGSCALED:AVERAGE:"%8.2lf%% average" GPRINT:cpu_util_steal_LEGSCALED:MAX:"%8.2lf%% max" GPRINT:cpu_util_steal_LEGSCALED:LAST:"%8.2lf%% last" COMMENT:"\n" CDEF:DERIVED5=user,system,io_wait,cpu_util_guest,cpu_util_steal,+,+,+,+ CDEF:DERIVED5_LEGSCALED=DERIVED5,1.000000,/ LINE:DERIVED5#004080:"Total                  " GPRINT:DERIVED5_LEGSCALED:AVERAGE:"%8.2lf%% average" GPRINT:DERIVED5_LEGSCALED:MAX:"%8.2lf%% max" GPRINT:DERIVED5_LEGSCALED:LAST:"%8.2lf%% last" COMMENT:"\n" 
