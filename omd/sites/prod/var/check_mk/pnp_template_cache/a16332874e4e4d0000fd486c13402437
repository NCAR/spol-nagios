Round trip average
 --vertical-label 'sec' --title 'Round trip average'  -l 0
DEF:rtmin_UNSCALED=$RRDBASE$_rtmin.rrd:1:MAX CDEF:rtmin=rtmin_UNSCALED,0.001000,* CDEF:rtmin_LEGSCALED=rtmin,1.000000,/ CDEF:rtmin_NEG=rtmin,-1,* CDEF:rtmin_LEGSCALED_NEG=rtmin_LEGSCALED,-1,* DEF:rta_UNSCALED=$RRDBASE$_rta.rrd:1:MAX CDEF:rta=rta_UNSCALED,0.001000,* CDEF:rta_LEGSCALED=rta,1.000000,/ CDEF:rta_NEG=rta,-1,* CDEF:rta_LEGSCALED_NEG=rta_LEGSCALED,-1,* DEF:rtmax_UNSCALED=$RRDBASE$_rtmax.rrd:1:MAX CDEF:rtmax=rtmax_UNSCALED,0.001000,* CDEF:rtmax_LEGSCALED=rtmax,1.000000,/ CDEF:rtmax_NEG=rtmax,-1,* CDEF:rtmax_LEGSCALED_NEG=rtmax_LEGSCALED,-1,* DEF:pl=$RRDBASE$_pl.rrd:1:MAX CDEF:pl_LEGSCALED=pl,1.000000,/ CDEF:pl_NEG=pl,-1,* CDEF:pl_LEGSCALED_NEG=pl_LEGSCALED,-1,* AREA:rtmax#6690ff:"Round trip maximum" LINE:rtmax#5173cc GPRINT:rtmax_LEGSCALED:AVERAGE:"%8.2lf s average" GPRINT:rtmax_LEGSCALED:MAX:"%8.2lf s max" GPRINT:rtmax_LEGSCALED:LAST:"%8.2lf s last" COMMENT:"\n" AREA:rtmin#0047ff:"Round trip minimum" LINE:rtmin#0038cc GPRINT:rtmin_LEGSCALED:AVERAGE:"%8.2lf s average" GPRINT:rtmin_LEGSCALED:MAX:"%8.2lf s max" GPRINT:rtmin_LEGSCALED:LAST:"%8.2lf s last" COMMENT:"\n" LINE:rta#40a0b0:"Round trip average" GPRINT:rta_LEGSCALED:AVERAGE:"%8.2lf s average" GPRINT:rta_LEGSCALED:MAX:"%8.2lf s max" GPRINT:rta_LEGSCALED:LAST:"%8.2lf s last" COMMENT:"\n" 
Packet loss
 --vertical-label '%' --title 'Packet loss'  -l 0
DEF:rtmin_UNSCALED=$RRDBASE$_rtmin.rrd:1:MAX CDEF:rtmin=rtmin_UNSCALED,0.001000,* CDEF:rtmin_LEGSCALED=rtmin,1.000000,/ CDEF:rtmin_NEG=rtmin,-1,* CDEF:rtmin_LEGSCALED_NEG=rtmin_LEGSCALED,-1,* DEF:rta_UNSCALED=$RRDBASE$_rta.rrd:1:MAX CDEF:rta=rta_UNSCALED,0.001000,* CDEF:rta_LEGSCALED=rta,1.000000,/ CDEF:rta_NEG=rta,-1,* CDEF:rta_LEGSCALED_NEG=rta_LEGSCALED,-1,* DEF:rtmax_UNSCALED=$RRDBASE$_rtmax.rrd:1:MAX CDEF:rtmax=rtmax_UNSCALED,0.001000,* CDEF:rtmax_LEGSCALED=rtmax,1.000000,/ CDEF:rtmax_NEG=rtmax,-1,* CDEF:rtmax_LEGSCALED_NEG=rtmax_LEGSCALED,-1,* DEF:pl=$RRDBASE$_pl.rrd:1:MAX CDEF:pl_LEGSCALED=pl,1.000000,/ CDEF:pl_NEG=pl,-1,* CDEF:pl_LEGSCALED_NEG=pl_LEGSCALED,-1,* AREA:pl#ffc030:"Packet loss" LINE:pl#cc9926 GPRINT:pl_LEGSCALED:AVERAGE:"%8.2lf%% average" GPRINT:pl_LEGSCALED:MAX:"%8.2lf%% max" GPRINT:pl_LEGSCALED:LAST:"%8.2lf%% last" COMMENT:"\n" 
